<mvc:View
	height="100%"
	controllerName="cmsfrontend.controller.consultingfirm.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page
		id="profile1"
		showHeader="true"
		showNavButton="true"
  >
    <customHeader>
      <core:Fragment
          fragmentName="cmsfrontend.view.fragments.NavBar"
          type="XML"
          />
      </customHeader>
    <content>
      <!-- core:Fragment fragmentName="cmsfrontend.fragments.profileDisplay" type="XML"> </core:Fragment-->
    </content>
		<footer>
			<Bar>
				<contentRight>
					<Button id="edit1" text="Edit" press="handleEditPress" type="Emphasized" width="15%"/>
					<Button id="save1" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
					<Button id="cancel1" text="Cancel" visible="false" press="handleCancelPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>
