<core:FragmentDefinition
     xmlns="sap.m"
     xmlns:core="sap.ui.core"
     >
  <Dialog id="idAccountTypeDialog" title="Account Type">
    <content>
      <SelectList
          itemPress="onPressItem"
          items="{account_types>/data}"
          >
        <core:Item key="{account_types>key}" text="{account_types>name}" />
      </SelectList>
    </content>
    <endButton>
      <Button text="Cancel" press="onPressCancel" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>
