<core:FragmentDefinition
     xmlns="sap.m"
     xmlns:core="sap.ui.core"
     >
  <Bar design="Header" >
    <contentLeft>
      <HBox>
        <Button
            icon="sap-icon://nav-back"
            press="onNavBackPress"
            visible="{config>/showBackButton}"
            />
      </HBox>
    </contentLeft>
    <contentMiddle>
      <Title text="{config>/navTitle}" />
    </contentMiddle>
    <contentRight>
      <HBox>
        <Button
            icon="sap-icon://log"
            press="onLogOutPress"
            />
      </HBox>
    </contentRight>
  </Bar>
</core:FragmentDefinition>
