<mvc:View
	height="100%"
	controllerName="cmsfrontend.controller.consultant.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page
		id="profile"
		showHeader="true"
		showNavButton="true" >
        <customHeader>
          <Bar
              design="Header"
              >
            <contentLeft>
              <HBox>
                <Button
                    icon="sap-icon://nav-back"
                    press="onNavBackPress"
                    />
              </HBox>
            </contentLeft>
            <contentMiddle>
              <Title text="{config>/navTitle}" />
            </contentMiddle>
            <contentRight>
              <HBox>
                <Button
                    icon="sap-icon://log"
                    press="onLogOutPress"
                    />
              </HBox>
            </contentRight>
          </Bar>
        </customHeader>
		<content>
			<!-- core:Fragment fragmentName="cmsfrontend.fragments.profileDisplay" type="XML"> </core:Fragment-->
		</content>
		<footer>

			<Bar>
				<contentRight>
					<Button id="edit" text="Edit" press="handleEditPress" />
					<Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
					<Button id="cancel" text="Cancel" visible="false" press="handleCancelPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>
